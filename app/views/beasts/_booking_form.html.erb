<%= simple_form_for([@beast, @booking], html: {class: "form-group form-box"}) do |f| %>

<%= f.label :start_date %>
<%= f.date_field :start_date, class: "form-control" %> <br>

<%= f.label :end_date %>
<%= f.date_field :end_date, class: "form-control" %> <br>

<%= f.submit class: "btn btn-primary" %>

<% end %>
