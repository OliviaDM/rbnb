<%= simple_form_for([@beast, @booking, @review], :url => review_path)  do |f| %>
<%= "beast review" %> <br>
<%= f.label :beast_rating %>
<%= f.number_field :beast_rating %> <br>

<%= f.label :beast_content %>
<%= f.text_field :beast_content %> <br>

<%= "Sucker review" %> <br>
<%= f.label :sucker_rating %>
<%= f.number_field :sucker_rating %> <br>

<%= f.label :sucker_content %>
<%= f.text_field :sucker_content %> <br>

<%= f.submit %>

<% end %>
